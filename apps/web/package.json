{
  "name": "@omnipdf/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --config=tests/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=tests/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=tests/playwright.config.ts --headed",
    "test:a11y": "playwright test --config=tests/playwright.config.ts --project=chromium tests/accessibility",
    "test:performance": "playwright test --config=tests/playwright.config.ts --project=chromium tests/performance",
    "test:integration": "playwright test --config=tests/playwright.config.ts tests/integration",
    "test:security": "playwright test --config=tests/playwright.config.ts tests/security",
    "test:compatibility": "playwright test --config=tests/playwright.config.ts tests/compatibility",
    "test:localization": "playwright test --config=tests/playwright.config.ts tests/localization",
    "test:usability": "playwright test --config=tests/playwright.config.ts tests/usability",
    "test:interruption": "playwright test --config=tests/playwright.config.ts tests/interruption",
    "test:recovery": "playwright test --config=tests/playwright.config.ts tests/recovery",
    "test:regression": "playwright test --config=tests/playwright.config.ts tests/regression",
    "test:install": "playwright test --config=tests/playwright.config.ts tests/installation"
  },
  "dependencies": {
    "@omnipdf/shared": "*",
    "@omnipdf/ui": "*",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/auth-helpers-react": "^0.15.0",
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-i18next": "^14.1.0",
    "tailwind-merge": "^2.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0"
  }
